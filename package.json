{
  "name": "monei-v2-dashboard",
  "version": "0.0.1",
  "description": "Monei Dashboard v2",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests; exit 0",
    "lint:fix": "eslint src tests --fix; exit 0",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "dev": "better-npm-run dev",
    "dev:prod": "better-npm-run dev:prod",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "build": "better-npm-run build",
    "build:prod": "better-npm-run build:prod",
    "deploy": "better-npm-run deploy",
    "deploy:prod": "better-npm-run deploy:prod",
    "release:patch": "npm version patch && npm run deploy",
    "release:minor": "npm version minor && npm run deploy",
    "release:major": "npm version major && npm run deploy",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "dev:prod": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "STAGE": "production",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "build:prod": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "STAGE": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run build -- -- --s3",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run build -- -- --s3",
      "env": {
        "NODE_ENV": "production",
        "STAGE": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "Ranier Montalbo",
  "license": "MIT",
